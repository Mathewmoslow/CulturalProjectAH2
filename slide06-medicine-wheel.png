<svg xmlns="http://www.w3.org/2000/svg" width="1200" height="1200" viewBox="0 0 1200 1200">
  <defs>
    <style>
      .title { font: 700 44px ui-sans-serif, system-ui, -apple-system, "Segoe UI", Roboto, Helvetica, Arial; fill: #0f1f26; }
      .label { font: 600 26px ui-sans-serif, system-ui, -apple-system, "Segoe UI", Roboto, Helvetica, Arial; fill: #0f1f26; letter-spacing:.2px; }
      .list { font: 400 22px ui-sans-serif, system-ui, -apple-system, "Segoe UI", Roboto, Helvetica, Arial; fill: #0f1f26; }
      .list.light { fill: #fcfcfa; }
      .small { font: 400 18px ui-sans-serif, system-ui, -apple-system, "Segoe UI", Roboto, Helvetica, Arial; fill: #5a6b74; }
    </style>
    <!-- soft drop shadow -->
    <filter id="softShadow" x="-20%" y="-20%" width="140%" height="140%">
      <feDropShadow dx="0" dy="8" stdDeviation="10" flood-color="#000000" flood-opacity="0.18"/>
    </filter>
    <!-- colors -->
    <linearGradient id="charcoalGrad" x1="0" y1="0" x2="1" y2="1">
      <stop offset="0%" stop-color="#1C1F22"/>
      <stop offset="100%" stop-color="#2A2F34"/>
    </linearGradient>
    <linearGradient id="whiteGrad" x1="0" y1="0" x2="1" y2="1">
      <stop offset="0%" stop-color="#FBFAF7"/>
      <stop offset="100%" stop-color="#F3F1EC"/>
    </linearGradient>
    <linearGradient id="yellowGrad" x1="0" y1="0" x2="1" y2="1">
      <stop offset="0%" stop-color="#F2C94C"/>
      <stop offset="100%" stop-color="#E5BE49"/>
    </linearGradient>
    <linearGradient id="redGrad" x1="0" y1="0" x2="1" y2="1">
      <stop offset="0%" stop-color="#D36A5C"/>
      <stop offset="100%" stop-color="#C65D52"/>
    </linearGradient>
  </defs>

  <rect width="100%" height="100%" fill="#F7F4EF"/>
  <text x="600" y="90" text-anchor="middle" class="title">Ojibwe Medicine Wheel</text>

  <!-- wheel group -->
  <g transform="translate(600 640)" filter="url(#softShadow)">
    <!-- base circle with copper stroke -->
    <circle cx="0" cy="0" r="420" fill="#ffffff" stroke="#B07A57" stroke-width="10"/>

    <!-- quadrants -->
    <!-- NE (East) - Yellow -->
    <path d="M0,0 L420,0 A420,420 0 0,1 0,-420 Z" fill="url(#yellowGrad)"/>
    <!-- SE (South) - Red -->
    <path d="M0,0 L0,420 A420,420 0 0,1 420,0 Z" fill="url(#redGrad)"/>
    <!-- SW (West) - Charcoal (modern 'black') -->
    <path d="M0,0 L-420,0 A420,420 0 0,1 0,420 Z" fill="url(#charcoalGrad)"/>
    <!-- NW (North) - White -->
    <path d="M0,0 L0,-420 A420,420 0 0,1 -420,0 Z" fill="url(#whiteGrad)"/>

    <!-- center ring -->
    <circle cx="0" cy="0" r="36" fill="#ffffff" stroke="#B07A57" stroke-width="6"/>
  </g>

  <!-- quadrant labels -->
  <!-- NORTH (top-left / white) -->
  <text x="210" y="330" class="label">North</text>
  <text x="210" y="365" class="list">winter</text>
  <text x="210" y="395" class="list">fire</text>
  <text x="210" y="425" class="list">elder</text>
  <text x="210" y="455" class="list">sweetgrass</text>

  <!-- EAST (top-right / yellow) -->
  <text x="780" y="330" class="label">East</text>
  <text x="780" y="365" class="list">spring</text>
  <text x="780" y="395" class="list">water</text>
  <text x="780" y="425" class="list">childhood</text>
  <text x="780" y="455" class="list">tobacco</text>

  <!-- SOUTH (bottom-right / red) -->
  <text x="780" y="980" class="label">South</text>
  <text x="780" y="1015" class="list">summer</text>
  <text x="780" y="1045" class="list">wind</text>
  <text x="780" y="1075" class="list">adolescence</text>
  <text x="780" y="1105" class="list">cedar</text>

  <!-- WEST (bottom-left / charcoal) -->
  <text x="210" y="980" class="label" fill="#fcfcfa">West</text>
  <text x="210" y="1015" class="list light">autumn</text>
  <text x="210" y="1045" class="list light">earth</text>
  <text x="210" y="1075" class="list light">adult</text>
  <text x="210" y="1105" class="list light">sage</text>

  <!-- legend (minimal) -->
  <text x="600" y="1180" text-anchor="middle" class="small">Muted palette with neutral charcoal “black” • copper ring • soft gradients • 1200×1200 SVG</text>
</svg>
